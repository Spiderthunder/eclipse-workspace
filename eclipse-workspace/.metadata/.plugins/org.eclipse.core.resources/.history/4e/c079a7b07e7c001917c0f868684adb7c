package Utility;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

public class Prova {

	public static void main(String[] args) {
		List<Double> miaLista=new ArrayList<Double>();
		miaLista.add(0, 0.3);//aggiungi valore
		System.out.println("lista:"+miaLista.toString());
		//miaLista.set(2, 0.4);//modifica valore
		//Double a=miaLista.get(0); //prende un valore lista

		//vettore----------------------------------

		Double[] vettoreDouble= new Double[4];
		vettoreDouble[0]=0.3;
		System.out.println("\nvettore:"+Arrays.toString(vettoreDouble));
		Double b= vettoreDouble[1];
		System.out.println("b"+b);

		Double[] vettoreDoubleR = {0.0,1.2,4.0,5.6};
		System.out.println(Arrays.toString(vettoreDoubleR));
	}

}
