package org.protor.sandbox.Aromano.tests;

import org.protor.sandbox.Aromano.UbstractVehicle;
import org.protor.sandbox.Aromano.VehicleType;

public class Test01 {

	public static void main(String[] args) {
		
		UbstractVehicle v1 = new UbstractVehicle(VehicleType.TERRESTRIAL);
		v1.setName("Fiat 600");
		
		UbstractVehicle v2 = new UbstractVehicle("Eurofighter", VehicleType.AERIAL);

		System.out.println("v1 - Name: " + v1.getName());
		System.out.println("v1 - Type: " + v1.getType());
		
		System.out.println("v2 - Name: " + v2.getName());
		System.out.println("v2 - Type: " + v2.getType());
		
	}

}
