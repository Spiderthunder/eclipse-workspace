package org.protor.sandbox.Aromano;

public class UbstractVehicle {
	
	private String name = "";
	private MyVehicleType type;
	
	public UbstractVehicle(MyVehicleType type) {
		this.type = type;
	}
	public UbstractVehicle(String name, MyVehicleType type) {
		this.name = name;
		this.type = type;
	}

	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}

	public MyVehicleType getType() {
		return type;
	}
	public void setType(MyVehicleType type) {
		this.type = type;
	}


}
